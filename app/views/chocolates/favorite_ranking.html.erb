<div class="container ranking">
  <div>
		<h2 class="toppage-oisisa">ChoCoBoXユーザーが選ぶ<br>いいね数ランキング</h2>
		<%= render 'chocolates/favorite', choco: @chocolate, chocolates: @chocolates, item_code: @item_code %>
  </div> 

	<div class="favo-page">
		<% if @items.present? %>
		<%= paginate @items, class: "favo-paginate" %>
		<% end %> 
	</div>

	<div class="yubi">
		<div id="back"><%= image_tag "arrow.png", size:'50x50' %></div>
	</div>
</div>
